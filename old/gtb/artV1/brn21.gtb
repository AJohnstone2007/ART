S ::=  'a' C | A C.
C ::= 'a' B .
B ::= # .
A ::= 'a' .


(

STRING := "aa"


(* Main Script *)
this_grammar := grammar[S] 
this_nfa := nfa[this_grammar lr 1 nullable_reductions]
this_dfa := dfa[this_nfa]
write[this_grammar]

render[open["dfa.vcg"] this_dfa]

this_derivation := brnglr_parse[this_dfa STRING]
render[open["s_brnglr.vcg"] this_derivation]

write["\n" CPU_time " CPU seconds elapsed\n\n"]
)

