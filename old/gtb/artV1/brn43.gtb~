 (* testing assigning of -ve numbers to packed nodes *)

S ::= 'b' S |  'b' 'b' | A | 'b' B .
A ::= 'b' 'b' 'b' .
B ::= 'b' 'b' .



 
(
this_grammar := grammar[S]
this_nfa := nfa[this_grammar lr 1 nullable_reductions]
lr1_dfa := dfa[this_nfa]
this_derivation := rnglr_parse[lr1_dfa "bbb"]
this_derivation := brnglr_parse[lr1_dfa "bbb"]
render[open["s_brnglr.vcg"] this_derivation]

)

