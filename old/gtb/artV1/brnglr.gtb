S ::= 'a'.

 
(
this_grammar := grammar[S]
this_nfa := nfa[this_grammar lr 1 nullable_reductions]
render[open["nfa.vcg"] this_nfa]
lr1_dfa := dfa[this_nfa]

write[this_grammar]
write[lr1_dfa]
this_derivation := brnglr_parse[lr1_dfa "a"]
render[open["ssg.vcg"] this_derivation]
